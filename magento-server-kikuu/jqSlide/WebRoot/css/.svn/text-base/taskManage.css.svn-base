
#wrapper {
	position:absolute; z-index:1;
	top:7%; bottom:0px; left:0;
	width:100%;
	background:#fafafa;
	overflow:auto;
}

#scroller {
	position:absolute; z-index:1;
/*	-webkit-touch-callout:none;*/
	-webkit-tap-highlight-color:rgba(0,0,0,0);
	width:100%;
	padding:0;
}
body,ul,li {
	padding:0;
	margin:0;
	border:0;
}
#scroller ul {
	list-style:none;
	padding:0;
	margin:0;
	width:100%;
	text-align:left;
}

#scroller li {
	padding:0 10px;
	height:35%;
	line-height:38px;
	border-bottom:1px solid #ccc;
	border-top:1px solid #fff;
	background-color:#fafafa;
}
p{ 
margin-bottom:0px;

}

